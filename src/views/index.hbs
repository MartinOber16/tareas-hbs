  <body class="text-center"> 
    <main class="form-signin">
      <form>
        <img class="mb-4" src="./assets/brand/tareas-icono.png" alt="" width="128" height="128">
        <h1 class="mb-3 fw-normal">Tareas</h1>

        <div class="input-group">
          <label for="inputEmail" class="visually-hidden">Email address</label>
          <input type="email" id="inputEmail" class="form-control" placeholder="Email address" required autofocus>
        </div>
        <div class="input-group">
          <label for="inputPassword" class="visually-hidden">Password</label>
          <input type="password" id="inputPassword" class="form-control" placeholder="Password" required>
        </div>
        <button class="w-100 btn btn-lg btn-success" id="buttonSubmit" type="submit"><i class="fa fa-sign-in" aria-hidden="true"></i> Ingresar</button>
      </form>
      <hr>
      <a href="usuarioGoogle"><button class="w-100 btn btn-lg btn-primary"><img class="" src="./assets/brand/google-icono.png" alt="" width="24" height="24"> Ingresar con <i class="fa fa-google" aria-hidden="true"></i>oogle</button></a>
      <hr>
